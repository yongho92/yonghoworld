@CHARSET "utf-8";

/*.body{overflow-y:hidden !important;}*/

/*헤더*/
#header{position:absolute; top:0; width:100%; height:90px; background: rgba(0,0,0,0.4); z-index:999;}
.head{width:1200px; margin:0 auto; overflow:hidden;}
.head h1{float:left; margin-top:19px;}
.head .menu{float:left; margin:30px 0 0 330px;}
.head .menu > ul{overflow:hidden;}
.head .menu ul li{float:left; margin-right:69px;}
.head .menu ul li:last-child{margin-right:0;}
.head .menu ul li a{font-size:18px; font-weight:700; color:#fff;}
.head .m_menu{display:none;}

.head .lang{float:right; margin-top:30px;}
.head .lang ul{}
.head .lang ul li{}
.head .lang ul li a{display:none; padding:5px 28px 5px 13px; margin-top:-1px; border:1px solid #abadaf; font-size:12px; color:#fff;}
.head .lang ul li a.on{display:block; margin-top:0; background:url(../../images/common/arrow.png) no-repeat 78% center;}

/*메인비쥬얼*/
.Allbox1{position:relative;}
.visual_slide{}
.visual_slide .slide1{width:100%; height:100%; overflow:hidden;}
.visual_slide .slide1{background:url(../../images/contents/visual1.jpg) no-repeat center center / cover;}
.visual_slide .slide1 .visual_txt{position:absolute; top:50%; left:50%; width:1200px; text-align:center; transform: translate(-50%, -50%);}
.visual_slide .slide1 .visual_txt .bord_box{width:650px; margin:0 auto; padding-bottom:30px; /*background:url(../../images/border.png) no-repeat 0 bottom;*/ border:6px solid rgba(255,255,255,0.5); border-top:none;}
.visual_slide .slide1 .visual_txt h2{font-size:56px; font-weight:700; font-family: 'Mont HeavyDEMO'; color:#fff; line-height: 97px;}
.visual_slide .slide1 .visual_txt h2 span{display:block; position:relative; line-height:10px;}
.visual_slide .slide1 .visual_txt h2 span:before{content:''; position:absolute; top:0; left:0; width:116px; height:6px; background:rgba(255,255,255,0.5);}
.visual_slide .slide1 .visual_txt h2 span:after{content:''; position:absolute; top:0; right:0; width:116px; height:6px; background:rgba(255,255,255,0.5);}
.visual_slide .slide1 .visual_txt p{margin-top:20px; font-size:18px; font-weight:400; color:#fff;}
.visual_slide .slide1 .visual_txt p span{display:block;}

/*사업영역*/
.content{width:1200px; height:100%; margin:0 auto;}
.Allbox2{background:url(../../images/contents/busi_bg.jpg) no-repeat center center / cover;}
.business{}
.business .busi_box{margin-top:40px;}
.business .busi_box .busi_l{float:left; width:580px; height:490px; text-align:center; overflow:hidden; box-shadow: 0 0 28px -14px #000;}
.business .busi_box .busi_l1{margin-right:40px; background:url(../../images/contents/busi_img1.png) no-repeat 0 0;}
.business .busi_box .busi_l2{background:url(../../images/contents/busi_img2.png) no-repeat 0 0;}
.business .busi_box .busi_l p{margin-top:180px; font-size:30px; font-family: 'Mont HeavyDEMO'; color:#fff; }
.business .busi_box .busi_l a{display:inline-block; height:70px; margin-top:10px; padding:0 20px; border:1px solid #fff; font-size:30px; font-weight:200; color:#fff; line-height:70px;}
.business .busi_box .busi_l a strong{}

/*고객센터*/
.Allbox3{background:url(../../images/contents/cut_bg.jpg) no-repeat center center / cover;}
.cut_list{margin-top:50px;}
.cut_list ul li{float:left; width:380px; margin-right:30px; text-align:center;}
.cut_list ul li + li + li{margin:0;}
.cut_list ul li .cut_box{}
.cut_list ul li .cut_box .cut_tit{height:343px; overflow:hidden;}
.cut_list ul li .cut_box .cut_tit1{background:url(../../images/contents/cut_img1.png) no-repeat 0 0;}
.cut_list ul li .cut_box .cut_tit2{background:url(../../images/contents/cut_img2.png) no-repeat 0 0;}
.cut_list ul li .cut_box .cut_tit3{background:url(../../images/contents/cut_img3.png) no-repeat 0 0;}
.cut_list ul li .cut_box .cut_tit p{position:relative; margin-top:70px; padding-top:135px; font-size:40px; font-weight:200; color:#fff;}
.cut_list ul li .cut_box .cut_tit p:before{content:''; position:absolute; top:0; left:180px; width:20px; height:20px; background:url(../../images/common/bar.png) no-repeat 0 0;}
.cut_list ul li .cut_box .cut_tit1 p{background:url(../../images/contents/icon1.png) no-repeat center 50%;}
.cut_list ul li .cut_box .cut_tit2 p{background:url(../../images/contents/icon2.png) no-repeat center 50%;}
.cut_list ul li .cut_box .cut_tit3 p{background:url(../../images/contents/icon3.png) no-repeat center 50%;}
.cut_list ul li .cut_box .cut_txt{display:table-cell; width:380px; height:150px; background:#e7e7e7; vertical-align:middle;}
.cut_list ul li .cut_box .cut_txt p{font-size:18px; font-weight:400; color:#222; line-height:28px;}
.cut_list ul li .cut_box .cut_txt p span{display:block;}
.cut_list ul li .cut_box .cut_txt2 p span{position:relative; display:inline; padding-left:18px;}
.cut_list ul li .cut_box .cut_txt2 p span:before{content:''; position:absolute; top:15px; left:0; width:8px; height:2px; background:#e73411;}

/*contact*/
.contact_a{width:100%; height:100%; background:url(../../images/contents/cont_bg.png) no-repeat center center / cover; overflow:hidden;}
.contact{position:relative; height:100%; overflow:hidden;}
.contact .map{position:absolute; top:0; left:50%; margin-top:60px; transform:translate(-50%, 0);}
.contact .pos_box{position:absolute; top:0; width:542px;}
.contact .pos_box .pin{position:relative; float:right; margin-top:20px;}
.contact .pos_box0 .pin{float:left;}
.contact .pos_box1{top:83px;}
.contact .pos_box2{top: 339px;}
.contact .pos_box3{right:0;top:135px;width: 690px;text-align:right;}
.contact .pos_box4{right:0;top: 239px;width: 680px;text-align:right;}
.contact .pos_box .pin:after{content:''; position:absolute; top:22px; height:1px; background:#aba2a0;}
.contact .pos_box1 .pin:after{content:'';left: -238px;width: 247px;}
.contact .pos_box2 .pin:after{content:'';left: -219px;width: 229px;}
.contact .pos_box3 .pin{margin-top: -20px;text-align:center;}
.contact .pos_box3 .pin .ps_comp{width:100px; height:30px; margin-top:4px; background:#e73411; line-height:27px;}
.contact .pos_box3 .pin .ps_comp img{width:59px;}
.contact .pos_box3 .pin .ps_comp span{font-size:12px; font-weight:700; color:#fff;}
.contact .pos_box3 .pin:after{content:''; top:59px; right: -274px; width: 274px;}
.contact .pos_box4 .pin:after{content:''; right:-323px; width:333px;}
.contact .comp_map{display:inline-block; padding:20px; border:1px solid #aba2a0;}
.contact .comp_map h5{font-size:20px; color:#222; text-align:left;}
.contact .comp_map a{float:right; margin-top:7px; font-size:12px; font-weight:400; color:#e73411;}
.contact .comp_map p{font-size:16px; font-weight:400; color:#888888;}
.contact .pos_box .comp_map.on{background:#e73411; border:none;}
.contact .pos_box .comp_map.on h5, .contact .pos_box .comp_map.on a{color:#fff;}
.contact .pos_box .comp_map.on p{color:#f3aea6;}

/*푸터*/
.foot_box{position:absolute; bottom:0; width:100%;}
#footer{ width:100%; padding:30px 0; background:#888888; overflow:hidden;}
.foot{width:1200px; height:100%; margin:0 auto; color:#fff; overflow:hidden;}
.foot .f_logo{float:left; margin-top:35px; font-size:18px; }
.foot .addr{float:left; margin:0 0 0 232px;}
.foot .addr ul li{line-height:25px; overflow:hidden;}
.foot .addr p{float:left; width:300px; font-size:12px; font-weight:400;}
.foot .addr p + p{width:auto;}
.foot .addr p + p + p{margin-left:20px;}
.foot .addr p em{margin-left:32px;}
.foot .addr .copy{display:block; margin-top:10px; font-size:10px; font-family: 'Montserrat'; font-weight:300;}
.foot .family{float:right; margin-top:35px;}
.foot .family ul{}
.foot .family ul li{}
.foot .family ul li.on a{padding:6px 30px 6px 19px; border:1px solid #abadaf; background:url(../../images/common/arrow.png) no-repeat 87% center; font-size:12px; font-family: 'Montserrat'; font-weight:600; color:#fff;}

/*카피*/
#copy{width:100%; padding:20px 0; background:#555555; text-align:center;}
#copy p{font-size:10px; font-family: 'Montserrat'; font-weight:300; color:#fff; letter-spacing:1px;}

/*스크롤버튼*/
.scroll{position:absolute; bottom:0; right:10%; height:100px; z-index:99; }
.scroll button{position:relative; background:none; font-size:12px; font-family: 'Montserrat'; font-weight:600; color:#fff; letter-spacing:2px; cursor:pointer;
-ms-transform: rotate(90deg);
-moz-transform: rotate(90deg);
-webkit-transform: rotate(90deg);
transform: rotate(90deg);
-ms-transform-origin: left top;
-moz-transform-origin: left top;
-webkit-transform-origin: left top;
transform-origin: left top;}
.scroll button:before{content:''; position:absolute; top:23px; right:-56px; width:2px; height:100px; background:#fff;
-ms-transform: rotate(90deg);
-moz-transform: rotate(90deg);
-webkit-transform: rotate(90deg);
transform: rotate(90deg);
-ms-transform-origin: left top;
-moz-transform-origin: left top;
-webkit-transform-origin: left top;
transform-origin: left top;}

.scroll2{position:absolute; bottom:0; right:10%; height:100px; z-index:99; }
.scroll2 button{position:relative; background:none; font-size:12px; font-family: 'Montserrat'; font-weight:600; color:#fff; letter-spacing:2px; cursor:pointer;
-ms-transform: rotate(90deg);
-moz-transform: rotate(90deg);
-webkit-transform: rotate(90deg);
transform: rotate(90deg);
-ms-transform-origin: left top;
-moz-transform-origin: left top;
-webkit-transform-origin: left top;
transform-origin: left top;}
.scroll2 button:before{content:''; position:absolute; top:23px; right:-56px; width:2px; height:100px; background:#fff;
-ms-transform: rotate(90deg);
-moz-transform: rotate(90deg);
-webkit-transform: rotate(90deg);
transform: rotate(90deg);
-ms-transform-origin: left top;
-moz-transform-origin: left top;
-webkit-transform-origin: left top;
transform-origin: left top;}

/*메인비쥬얼 페이징버튼*/
.swiper-pagination{left:50%; bottom:40px; width:1200px; text-align:center !important; transform:translate(-50%, -50%) !important;}
.swiper-pagination-bullet{width:50px; height:4px; background:#fff; border-radius:0px;}
.s_Btn{position:absolute; top:55%; left:50%; width:1200px; z-index:999; transform:translate(-50%, -50%);}
.s_Btn .swiper-button-next, .swiper-button-prev{width:50px; height:50px; background:url(../../images/common/arrow2.png) no-repeat 0 0;}
.s_Btn .swiper-button-next{background-position:-60px 0; right:0;}

.s_Btn2{width:100%; top:51%;}
.s_Btn2 .swiper-button-next, .s_Btn2 .swiper-button-prev{width:40px; height:40px; background-size:88px;}
.s_Btn2 .swiper-button-prev{left:30px;}
.s_Btn2 .swiper-button-next{background-position:-48px 0; right:30px;}

/*공통*/
.Allbox{position:relative; width:100%; height:100vh; overflow:hidden;}
.Allbox h3{padding-top:60px; font-size:46px; font-family: 'Mont ExtraLight'; font-weight:100; color:#e73411; text-align:center;}
.Allbox h3 strong{font-family: 'Mont HeavyDEMO'; color:#e73411;}
.Allbox h3 .color1{color:#fff;}
.Allbox h4{font-size:48px; font-weight:100; color:#e73411; text-align:center; }
.content .cont_tit{margin-top:50px; text-align:center;}
.content .cont_tit dt{font-size:30px; font-family: 'Mont ExtraLight'; font-weight:normal; color:#222; }
.content .cont_tit dd{font-size:24px; font-weight:100; color:#222;}
.content .cont_tit2 dt{color:#fff; }
.content .cont_tit2 dd{color:#fff;}

/*모바일메뉴*/
